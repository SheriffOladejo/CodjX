{
	"comments": {
		"lineComment": "#",
		"blockComment": [ "<#", "#>" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		{ "open": "[", "close": "]" },
		{ "open": "{", "close": "}" },
		{ "open": "(", "close": ")" },
		{ "open": "@'", "close": "\n'@", "notIn": ["string", "comment"]},
		{ "open": "@\"", "close": "\n\"@", "notIn": ["string", "comment"]},
		{ "open": "\"", "close": "\"", "notIn": ["string"]},
		{ "open": "'", "close": "'", "notIn": ["string", "comment"]},
		{ "open": "<#", "close": "#>" }
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"]
	],
	"folding": {
		"markers": {
			"start": "^\\s*#[rR]egion\\b",
			"end": "^\\s*#[eE]nd[rR]egion\\b"
		}
	}
}